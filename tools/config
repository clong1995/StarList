alias nb='npm run build'
alias fs='fie start'
alias ni='npm i'

# git config 
alias gs='git status'
alias gm='git merge'
alias gc='git commit -m'
alias gl='git log'
alias ga='git add .'
alias gp='git push'
alias gd='git diff'
alias gb='git branch'
alias gr='git reset'
alias gf='git fetch'
alias gr='git remote'
